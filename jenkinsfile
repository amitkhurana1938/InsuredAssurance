pipeline {
  agent any

  tools {
      maven "mvn3.9.8"
 }
 stages {
     stage('Checkout') {
        steps {
          git 'https://github.com/amitkhurana1938/InsuredAssurance.git'
        }
	  }
     stage('Build'){
	 steps{
            sh 'mvn clean package'
          }
     }
     stage('Test') {
         steps {
		echo 'testing done'
         }
     }
       stage('Deploy') {
	      steps {
		echo 'Deploy done'
        }
	  }
}
post {
  success {
  echo 'Job executed'
}
  failure {
   echo 'Failed'
}
}
}
